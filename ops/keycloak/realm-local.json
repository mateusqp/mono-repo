{
  "realm": "app-local",
  "enabled": true,
  "displayName": "Aplicação Local",
  "loginTheme": "keycloak",
  "registrationAllowed": false,
  "rememberMe": true,
  "resetPasswordAllowed": true,
  "clients": [
    {
      "clientId": "app-frontend",
      "name": "Frontend local",
      "protocol": "openid-connect",
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "pkceEnforce": true,
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:5173/*"
      },
      "redirectUris": [
        "http://localhost:5173/*"
      ],
      "webOrigins": [
        "http://localhost:5173"
      ],
      "defaultClientScopes": [
        "web-origins",
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    },
    {
      "clientId": "app-backend",
      "name": "Backend local",
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "backend-secret",
      "serviceAccountsEnabled": false,
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "bearerOnly": true,
      "authorizationServicesEnabled": false,
      "redirectUris": [],
      "webOrigins": [],
      "protocolMappers": [
        {
          "name": "app-backend audience",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "app-backend",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ],
      "defaultClientScopes": [
        "profile",
        "roles",
        "email"
      ],
      "optionalClientScopes": [
        "address",
        "phone",
        "offline_access",
        "microprofile-jwt"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "app-admin",
        "description": "Administrador da aplicação local",
        "composite": false,
        "clientRole": false,
        "containerId": "app-local"
      }
    ]
  },
  "defaultDefaultClientScopes": [
    "web-origins",
    "profile",
    "roles",
    "email"
  ],
  "defaultOptionalClientScopes": [
    "address",
